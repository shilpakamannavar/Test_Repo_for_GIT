<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">auraine_pushnotification_subscriber_listing.auraine_pushnotification_subscriber_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>auraine_pushnotification_subscriber_columns</spinner>
		<deps>
			<dep>auraine_pushnotification_subscriber_listing.auraine_pushnotification_subscriber_listing_data_source</dep>
		</deps>
		<buttons>
			<button name="add">
				<url path="*/*/new"/>
				<class>primary</class>
				<label translate="true">Add new Subscriber</label>
			</button>
		</buttons>
	</settings>
	<dataSource name="auraine_pushnotification_subscriber_listing_data_source" component="Magento_Ui/js/grid/provider">
		<settings>
			<storageConfig>
				<param name="indexField" xsi:type="string">subscriber_id</param>
			</storageConfig>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Auraine_PushNotification::Subscriber</aclResource>
		<dataProvider name="auraine_pushnotification_subscriber_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>subscriber_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>true</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="auraine_pushnotification_subscriber_columns">
		<settings>
			<editorConfig>
				<param name="selectProvider" xsi:type="string">auraine_pushnotification_subscriber_listing.auraine_pushnotification_subscriber_listing.auraine_pushnotification_subscriber_columns.ids</param>
				<param name="enabled" xsi:type="boolean">true</param>
				<param name="indexField" xsi:type="string">subscriber_id</param>
				<param name="clientConfig" xsi:type="array">
					<item name="saveUrl" xsi:type="url" path="auraine_pushnotification/Subscriber/inlineEdit"/>
					<item name="validateBeforeSave" xsi:type="boolean">false</item>
				</param>
			</editorConfig>
			<childDefaults>
				<param name="fieldAction" xsi:type="array">
					<item name="provider" xsi:type="string">auraine_pushnotification_subscriber_listing.auraine_pushnotification_subscriber_listing.auraine_pushnotification_subscriber_columns_editor</item>
					<item name="target" xsi:type="string">startEdit</item>
					<item name="params" xsi:type="array">
						<item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
						<item name="1" xsi:type="boolean">true</item>
					</item>
				</param>
			</childDefaults>
		</settings>
		<selectionsColumn name="ids">
			<settings>
				<indexField>subscriber_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="subscriber_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="customer_id">
			<settings>
				<filter>text</filter>
				<label translate="true">customer_id</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<actionsColumn name="actions" class="Auraine\PushNotification\Ui\Component\Listing\Column\SubscriberActions">
			<settings>
				<indexField>subscriber_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>
		<column name="date_of_birth">
			<settings>
				<filter>text</filter>
				<label translate="true">date_of_birth</label>
				<editor>
					<editorType>date</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="gender">
			<settings>
				<filter>text</filter>
				<label translate="true">gender</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="device_type">
			<settings>
				<filter>text</filter>
				<label translate="true">device_type</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="token">
			<settings>
				<filter>text</filter>
				<label translate="true">token</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="browser">
			<settings>
				<filter>text</filter>
				<label translate="true">browser</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="created_date">
			<settings>
				<filter>text</filter>
				<label translate="true">created_date</label>
				<editor>
					<editorType>date</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="ip_address">
			<settings>
				<filter>text</filter>
				<label translate="true">ip_address</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="entity_id">
			<settings>
				<filter>text</filter>
				<label translate="true">entity_id</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="notification_type">
			<settings>
				<filter>text</filter>
				<label translate="true">notification_type</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
	</columns>
</listing>
