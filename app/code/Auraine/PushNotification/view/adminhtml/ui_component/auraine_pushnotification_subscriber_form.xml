<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">auraine_pushnotification_subscriber_form.subscriber_form_data_source</item>
		</item>
		<item name="label" xsi:type="string" translate="true">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button name="back" class="Auraine\PushNotification\Block\Adminhtml\Subscriber\Edit\BackButton"/>
			<button name="delete" class="Auraine\PushNotification\Block\Adminhtml\Subscriber\Edit\DeleteButton"/>
			<button name="save" class="Auraine\PushNotification\Block\Adminhtml\Subscriber\Edit\SaveButton"/>
			<button name="save_and_continue" class="Auraine\PushNotification\Block\Adminhtml\Subscriber\Edit\SaveAndContinueButton"/>
		</buttons>
		<namespace>auraine_pushnotification_subscriber_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>auraine_pushnotification_subscriber_form.subscriber_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="subscriber_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider name="subscriber_form_data_source" class="Auraine\PushNotification\Model\Subscriber\DataProvider">
			<settings>
				<requestFieldName>subscriber_id</requestFieldName>
				<primaryFieldName>subscriber_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field name="customer_id" formElement="input" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Subscriber</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">customer_id</label>
				<dataScope>customer_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="date_of_birth" formElement="date" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Subscriber</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">date_of_birth</label>
				<dataScope>date_of_birth</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="gender" formElement="input" sortOrder="30">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Subscriber</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">gender</label>
				<dataScope>gender</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="device_type" formElement="input" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Subscriber</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">device_type</label>
				<dataScope>device_type</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="token" formElement="textarea" sortOrder="50">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Subscriber</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">token</label>
				<dataScope>token</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="browser" formElement="textarea" sortOrder="60">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Subscriber</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">browser</label>
				<dataScope>browser</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="created_date" formElement="date" sortOrder="70">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Subscriber</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">created_date</label>
				<dataScope>created_date</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="ip_address" formElement="textarea" sortOrder="80">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Subscriber</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">ip_address</label>
				<dataScope>ip_address</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="entity_id" formElement="input" sortOrder="90">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Subscriber</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">entity_id</label>
				<dataScope>entity_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field name="notification_type" formElement="textarea" sortOrder="170">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Subscriber</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">notification_type</label>
				<dataScope>notification_type</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
