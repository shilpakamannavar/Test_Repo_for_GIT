<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="Auraine_images" resource="default" engine="innodb" comment="Auraine_images Table">
		<column xsi:type="int" name="image_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Image Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="image_id"/>
		</constraint>
		<column name="path" nullable="true" xsi:type="text" comment="path"/>
		<column name="name" nullable="true" xsi:type="text" comment="name"/>
		</table>
</schema>